for i in $(seq 10); do letters=$(for letter in {a..z}; do echo $letter; done | tr '\n' ' '); echo $i | query=$(awk -v letters="$letters" -v v_index="$i" "BEGIN {srand(v_index)}; {split(letters, list_letters); for(i =0; i<(rand()*100 % 5)); i++) r_word=r_word list_letters[int(rand()*length(list_letters))+1]; print r_word}; printf("(%d %s %d %d)\", v_index, r_word, 15, 2)}"); echo $query; done
